#lang racket

;;; Program to find if a number is power of 2
;;WORKS

(define (power-of num)
  (cond
    ;;If the entered number has a remaider of 0 when divided by
    ;; 2, it will be a power of 2
    [(=(modulo num 2)0) "Is a power of 2"]
    ;;Otherwise it isnt a power of 2
    [else "Isn't a power of 2"]))